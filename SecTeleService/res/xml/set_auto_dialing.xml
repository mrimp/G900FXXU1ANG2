        <Preference android:persistent="false" android:title="@string/lgt_service_global_auto_roaming_country_number" android:key="country_code" android:summary="@string/lgt_service_global_auto_roaming_country_number" android:dependency="toggle_auto_dialing" />
    </PreferenceCategory>
    <PreferenceScreen android:title="@string/auto_dialing_guide" android:key="auto_dialing_guide" />
</PreferenceScreen>